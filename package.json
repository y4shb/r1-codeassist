{
  "name": "r1-codeassist",
  "displayName": "r1-codeassist",
  "description": "A DeepSeek-R1 powered Coding Assistant that plugs into VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "deepseek-chat",
          "title": "DeepSeek Chat",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "views": {
      "deepseek-chat": [
        {
          "type": "webview",
          "id": "deepChatView",
          "name": "DeepSeek Chat"
        }
      ]
    },
    "commands": [
      {
        "command": "r1-codeassist.deepChat",
        "title": "DeepSeek Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "ollama": "^0.5.12"
  }
}
